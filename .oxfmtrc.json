{
  "$schema": "./node_modules/oxfmt/configuration_schema.json",
  "printWidth": 80,
  "singleQuote": true,
  "semi": false,
  "objectWrap": "preserve",
  "quoteProps": "as-needed",
  "experimentalSortPackageJson": true,
  "experimentalSortImports": {
    "newlinesBetween": false,
    "enabled": true
  },
  "ignorePatterns": [
    ".next/",
    ".yarn/",
    "public/",
    ".yarnrc.yml",
    "next-env.d.ts"
  ]
}
