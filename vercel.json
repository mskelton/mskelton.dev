{
  "github": {
    "silent": true
  },
  "rewrites": [
    {
      "source": "/yarn-outdated/:version*",
      "destination": "https://raw.githubusercontent.com/mskelton/yarn-plugin-outdated/:version*/bundles/@yarnpkg/plugin-outdated.js"
    },
    {
      "source": "/ratchet.ts",
      "destination": "https://raw.githubusercontent.com/mskelton/ratchet/main/transform.ts"
    },
    {
      "source": "/ratchet",
      "destination": "https://ratchet-mskelton.vercel.app"
    }
  ]
}
