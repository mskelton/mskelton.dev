{
  "github": {
    "silent": true
  },
  "redirects": [
    {
      "source": "/username",
      "destination": "/blog/my-experience-changing-my-username"
    },
    {
      "source": "/shallow",
      "destination": "/blog/the-dangers-of-shallow-rendering"
    },
    {
      "source": "/env-vars",
      "destination": "/blog/why-i-dont-use-environment-variables"
    },
    {
      "source": "/npm-publishing",
      "destination": "/blog/automated-npm-publishing-using-github-actions"
    }
  ],
  "rewrites": [
    {
      "source": "/yarn-outdated/:version*",
      "destination": "https://raw.githubusercontent.com/mskelton/yarn-plugin-outdated/:version*/bundles/@yarnpkg/plugin-outdated.js"
    },
    {
      "source": "/ratchet.ts",
      "destination": "https://raw.githubusercontent.com/mskelton/ratchet/main/transform.ts"
    },
    {
      "source": "/ratchet/:asset*",
      "destination": "https://ratchet-mskelton.vercel.app/:asset*"
    }
  ]
}
